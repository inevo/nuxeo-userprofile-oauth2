		<releases>
			<enabled>true</enabled>
		</releases>
		<snapshots>
			<enabled>false</enabled>
		</snapshots>
	</repository>
	<repository>
		<id>public-snapshot</id>
		<url>http://maven.nuxeo.org/nexus/content/groups/public-snapshot
		</url>
		<releases>
			<enabled>false</enabled>
		</releases>
		<snapshots>
			<updatePolicy>always</updatePolicy>
			<enabled>true</enabled>
		</snapshots>
	</repository>
  </repositories>
